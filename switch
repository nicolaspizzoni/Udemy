//Comando Switch = fluxo de código com comparação de identicos, bom para utilização em menus

var opcao = prompt('Digite');
var a = parseInt(opcao);
switch(a) {
case 1:
document.write(opcao);
break;
case 2:
document.write(opcao);
break;
case 3:
document.write(opcao);
break;
default:
document.write('Wrong');
}
